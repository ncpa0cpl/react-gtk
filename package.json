{
  "name": "react-gnome",
  "packageManager": "yarn@3.2.4",
  "bin": "./bin/react-gnome.sh",
  "main": "./dist/legacy/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs"
    },
    "./build": {
      "types": "./dist/types/build.d.ts",
      "import": "./dist/esm/build.mjs",
      "require": "./dist/cjs/build.cjs"
    },
    "./polyfills/*": "./polyfills/esm/*.mjs"
  },
  "devDependencies": {
    "@ncpa0cpl/nodepack": "^2.0.1",
    "@swc/core": "^1.3.21",
    "@swc/jest": "^0.2.23",
    "@types/rimraf": "^3.0.2",
    "@types/tar": "^6.1.3",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "ncpa0cpl": "ncpa0cpl/git-hook-tasks",
    "prettier": "^2.8.0",
    "prettier-plugin-jsdoc": "^0.4.2",
    "typescript": "^4.9.3"
  },
  "version": "1.0.0",
  "scripts": {
    "fix:lint": "eslint --fix .",
    "fix:prettier": "prettier -w ./src .",
    "test:unit": "gjs -m ./gest/dist/esm/gest.mjs --verbose",
    "test:lint": "eslint .",
    "test:prettier": "prettier -c ./src && prettier -c ./__tests__",
    "test:tsc": "tsc --noEmit",
    "build": "node ./scripts/build.cjs",
    "build:gest": "node ./scripts/build-gest.cjs",
    "postinstall": "node ./dist/legacy/add-config-schema.js"
  },
  "keywords": [],
  "repository": {
    "url": ""
  },
  "description": "",
  "license": "MIT",
  "author": {
    "name": "Szymon Bretner",
    "email": ""
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "chalk": "^4.1.2",
    "clify.js": "ncpa0cpl/clify.js#0.0.1",
    "dilswer": "1.5.0",
    "esbuild": "^0.15.18",
    "path-browserify": "^1.0.1",
    "rimraf": "^3.0.2",
    "tar": "^6.1.13",
    "whatwg-url-without-unicode": "^8.0.0-3"
  },
  "peerDependencies": {
    "ts-node": "^10.9.0"
  }
